{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"hail-hail": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/hail-hail",
			"sourceRoot": "projects/hail-hail/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/hail-hail",
						"index": "projects/hail-hail/src/index.html",
						"main": "projects/hail-hail/src/main.ts",
						"polyfills": "projects/hail-hail/src/polyfills.ts",
						"tsConfig": "projects/hail-hail/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/hail-hail/src/favicon.ico",
							"projects/hail-hail/src/assets"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"src/styles"
							]
						},
						"styles": [
							"node_modules/bootstrap/dist/css/bootstrap.min.css",
							"projects/hail-hail/src/styles/main.scss"
						],
						"scripts": [
							"node_modules/jquery/dist/jquery.min.js",
							"node_modules/bootstrap/dist/js/bootstrap.min.js"
						]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "hail-hail:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "hail-hail:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "hail-hail:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/hail-hail/src/test.ts",
						"polyfills": "projects/hail-hail/src/polyfills.ts",
						"tsConfig": "projects/hail-hail/tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"codeCoverageExclude": [
							"projects/hail-hail/src/polyfills.ts",
							"projects/hail-hail/src/test.ts"
						],
						"assets": [
							"projects/hail-hail/src/favicon.ico",
							"projects/hail-hail/src/assets"
						],
						"styles": [
							"projects/hail-hail/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/hail-hail/tsconfig.app.json",
							"projects/hail-hail/tsconfig.spec.json",
							"projects/hail-hail/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/hail-hail/e2e/protractor.conf.js",
						"devServerTarget": "hail-hail:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "hail-hail:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "football-news"
}
